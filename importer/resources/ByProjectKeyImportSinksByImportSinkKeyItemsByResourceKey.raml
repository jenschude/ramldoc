# Resource
(resourceName): ByProjectKeyImportSinksByImportSinkKeyItemsByResourceKey
(resourcePathUri): /{projectKey}/import-sinks/{importSinkKey}/items/{resourceKey}

uriParameters:
  projectKey:
    type: string
    (builtinType): string
    description: |-
      The CTP project key.
    required: true
  importSinkKey:
    type: string
    (builtinType): string
    required: true
  resourceKey:
    type: string
    (builtinType): string
    required: true
get:
  description: |-
    Retrieves inidividual import items for the given resource and import sink.
  responses:
    200:
      body:
        application/json:
          type: ImportItem
          (builtinType): object