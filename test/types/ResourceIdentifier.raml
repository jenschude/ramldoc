#%RAML 1.0 DataType
displayName: ResourceIdentifier
type: object
(builtinType): object
discriminator: typeId
(oneOf):
- ExampleTargetTypeResourceIdentifier
properties:
  typeId:
    type: ReferenceTypeId
    (builtinType): string
    description: |-
      The allowed typeIds of the referenced resource.
    enum:
    - example-target
    - cart-discount
    - category
    - channel
    - customer
    - customer-group
    - price
    - product
    - product-type
    - product-variant
    - shipping-method
    - state
    - tax-category
    required: false
  id:
    type: string
    (builtinType): string
    required: false
  key:
    type: string
    (builtinType): string
    required: false