#%RAML 1.0 DataType
displayName: ZoneRate
type: object
(builtinType): object
(package): "ShippingMethod"
(docs-uri): "https://docs.commercetools.com/http-api-projects-shippingMethods.html#zonerate"
properties:
  zone:
    type: ZoneReference
    (builtinType): object
    required: true
  shippingRates:
    type: array
    items:
      type: ShippingRate
    (builtinType): array
    description: |-
      The array does not contain two shipping rates with the same currency.
    required: true