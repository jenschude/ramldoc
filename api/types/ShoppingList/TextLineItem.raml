#%RAML 1.0 DataType
type: object
displayName: TextLineItem


properties:
   addedAt:
      description: |-
        When the text line item was added to the shopping list.
      format: rfc3339
      type: datetime
   custom:
      type: CustomFields
   description:
      type: LocalizedString
   id:
      description: |-
        The unique ID of this TextLineItem.
      type: string
   name:
      type: LocalizedString
   quantity:
      format: int16
      type: number