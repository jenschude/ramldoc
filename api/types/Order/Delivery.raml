#%RAML 1.0 DataType
displayName: Delivery
type: object
(builtinType): object
properties:
  id:
    type: string
    (builtinType): string
    required: true
  createdAt:
    format: rfc3339
    type: datetime
    (builtinType): datetime
    required: true
  items:
    type: array
    items:
      type: DeliveryItem
    (builtinType): array
    description: |-
      Items which are shipped in this delivery regardless their distribution over several parcels.
      Can also be specified individually for each Parcel.
    required: true
  parcels:
    type: array
    items:
      type: Parcel
    (builtinType): array
    required: true
  address:
    type: Address
    (builtinType): object
    required: false