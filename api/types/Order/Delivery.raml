#%RAML 1.0 DataType
type: object
displayName: Delivery


properties:
   id:
      type: string
   createdAt:
      format: rfc3339
      type: datetime
   items:
      description: |-
        Items which are shipped in this delivery regardless their distribution over several parcels.
        Can also be specified individually for each Parcel.
      type: array
      items:
        type: DeliveryItem
   parcels:
      type: array
      items:
        type: Parcel
   address:
      type: Address