  paymentState:
    type: ReturnPaymentState
    (builtinType): string
    enum:
    - NonRefundable
    - Initial
    - Refunded
    - NotRefunded
    required: true
  lastModifiedAt:
    type: datetime
    (builtinType): datetime
    required: true
  createdAt:
    type: datetime
    (builtinType): datetime
    required: true