(docs-uri): "https://docs.commercetools.com/http-api-projects-orders.html#returnitem"
properties:
  id:
    type: string
    (builtinType): string
    required: true
  quantity:
    format: int64
    type: integer
    (builtinType): integer
    required: true
  type:
