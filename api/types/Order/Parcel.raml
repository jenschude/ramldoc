#%RAML 1.0 DataType
type: object
displayName: Parcel


properties:
   id:
      type: string
   createdAt:
      format: rfc3339
      type: datetime
   measurements:
      type: ParcelMeasurements
   trackingData:
      type: TrackingData
   items:
      description: |-
        The delivery items contained in this parcel.
      type: array
      items:
        type: DeliveryItem