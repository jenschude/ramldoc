#%RAML 1.0 DataType
type: object
(originalType): object
displayName: OrderEditDraft


properties:
  dryRun:
    required: false
    description: |-
      When set to `true` the edit is applied on the Order without persisting it.
    type: boolean
  resource:
    required: true
    description: |-
      The order to be updated with this edit.
    type: OrderReference
  custom:
    required: false
    description: |-
      The custom fields.
    type: CustomFieldsDraft
  comment:
    required: false
    description: |-
      This field can be used to add additional textual information regarding the edit.
    type: string
  stagedActions:
    required: false
    description: |-
      The actions to apply to `resource`.
    type: array
    items:
      type: StagedOrderUpdateAction
  key:
    required: false
    description: |-
      Unique identifier for this edit.
    type: string