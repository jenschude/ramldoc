#%RAML 1.0 DataType
displayName: CustomObjectDraft
type: object
(builtinType): object
properties:
  container:
    type: string
    (builtinType): string
    description: |-
      A namespace to group custom objects.
    required: true
  key:
    type: string
    (builtinType): string
    description: |-
      A user-defined key that is unique within the given container.
    required: true
  value:
    type: any
    (builtinType): any
    required: true
  version:
    format: int64
    type: integer
    (builtinType): integer
    required: false