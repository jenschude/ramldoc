#%RAML 1.0 DataType
displayName: CustomObject
type: LoggedResource
(builtinType): object
properties:
  id:
    type: string
    (builtinType): string
    (identifier): true
    required: true
  version:
    format: int64
    type: number
    (builtinType): number
    required: true
  createdAt:
    format: rfc3339
    type: datetime
    (builtinType): datetime
    required: true
  lastModifiedAt:
    format: rfc3339
    type: datetime
    (builtinType): datetime
    required: true
  lastModifiedBy:
    type: LastModifiedBy
    (builtinType): object
    required: false
  createdBy:
    type: CreatedBy
    (builtinType): object
    required: false
  container:
    type: string
    (builtinType): string
