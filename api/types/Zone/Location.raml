#%RAML 1.0 DataType
type: object
(originalType): object
displayName: Location
# 
# 
properties:
  country:
    required: true
    description: |-
      A two-digit country code as per [ISO 3166-1 alpha-2](https://en.wikipedia.org/wiki/ISO_3166-1_alpha-2).
    pattern: ^[A-Z]{2}$
    type: CountryCode
  state:
    required: false
    type: string