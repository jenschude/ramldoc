#%RAML 1.0 DataType
displayName: Location
type: object
(builtinType): object
(package): "Zone"
(docs-uri): "https://docs.commercetools.com/http-api-projects-zones.html#location"
properties:
  country:
    pattern: "^[A-Z]{2}$"
    type: CountryCode
    (builtinType): string
    description: |-
      A two-digit country code as per [ISO 3166-1 alpha-2](https://en.wikipedia.org/wiki/ISO_3166-1_alpha-2).
    required: true
  state:
    type: string
    (builtinType): string
    required: false