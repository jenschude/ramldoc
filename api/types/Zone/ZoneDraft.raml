#%RAML 1.0 DataType
type: object
(originalType): object
displayName: ZoneDraft
# 
# 
properties:
  name:
    required: true
    type: string
  description:
    required: false
    type: string
  locations:
    required: true
    type: array
    items:
      type: Location
  key:
    required: false
