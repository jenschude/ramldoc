#%RAML 1.0 DataType
type: object
displayName: TypedMoney
discriminator: type

properties:
   type:
      type: MoneyType
   fractionDigits:
      format: int32
      maximum: 12
      type: number
   centAmount:
      format: int64
      type: number
   currencyCode:
      description: |-
        The currency code compliant to [ISO 4217](https://en.wikipedia.org/wiki/ISO_4217).
        
      type: CurrencyCode