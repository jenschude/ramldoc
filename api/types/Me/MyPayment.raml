#%RAML 1.0 DataType
type: object
(originalType): object
displayName: MyPayment


properties:
  anonymousId:
    description: |-
      Identifies payments belonging to an anonymous session (the customer has not signed up/in yet).
    type: string
  paymentMethodInfo:
    type: PaymentMethodInfo
  custom:
    type: CustomFields
  amountPlanned:
    description: |-
      How much money this payment intends to receive from the customer.
      The value usually matches the cart or order gross total.
    discriminator: type
    type: TypedMoney
  id:
    type: string
  transactions:
    description: |-
      A list of financial transactions of different TransactionTypes
      with different TransactionStates.
    type: array
    items:
      type: Transaction
  version:
    format: int64
    type: number
  customer:
    description: |-
      A reference to the customer this payment belongs to.
    discriminatorValue: customer
    type: CustomerReference