#%RAML 1.0 DataType
type: object
(originalType): object
displayName: MyCartDraft


properties:
  lineItems:
    required: false
    type: array
    items:
      type: MyLineItemDraft
  country:
    required: false
    description: |-
      A two-digit country code as per [ISO 3166-1 alpha-2](https://en.wikipedia.org/wiki/ISO_3166-1_alpha-2).
    type: string
  itemShippingAddresses:
    required: false
    description: |-
      Contains addresses for orders with multiple shipping addresses.
      Each address must contain a key which is unique in this cart.
    type: array
    items:
      type: Address
  taxMode:
    required: false
    description: |-
      The `TaxMode` `Disabled` can not be set on the My Carts endpoint.
    type: TaxMode
  deleteDaysAfterLastModification:
    required: false
    description: |-
      The cart will be deleted automatically if it hasn't been modified for the specified amount of days and it is in the `Active` CartState.
      If a ChangeSubscription for carts exists, a `ResourceDeleted` notification will be sent.
    format: int64
    type: number
  custom:
    required: false
    description: |-
      The custom fields.
    type: CustomFieldsDraft
  shippingMethod:
    required: false
    type: ShippingMethodResourceIdentifier
  customerEmail:
    required: false
    type: string
  shippingAddress:
    required: false
    type: Address
  currency:
    required: true
    description: |-
      A three-digit currency code as per [ISO 3166-1 alpha-2](https://en.wikipedia.org/wiki/ISO_3166-1_alpha-2).
    pattern: ^[A-Z]{3}$
    type: CurrencyCode
  billingAddress:
    required: false
    type: Address
  locale:
    required: false
    type: string
  inventoryMode:
    required: false
    description: |-
      Default inventory mode is `None`.
    type: InventoryMode