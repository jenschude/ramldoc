#%RAML 1.0 DataType
type: object
(originalType): object
displayName: TaxRateDraft


properties:
  country:
    required: true
    description: |-
      A two-digit country code as per [ISO 3166-1 alpha-2](https://en.wikipedia.org/wiki/ISO_3166-1_alpha-2).
    pattern: ^[A-Z]{2}$
    type: CountryCode
  amount:
    required: false
    description: |-
      Percentage in the range of [0..1].
      Must be supplied if no `subRates` are specified.
      If `subRates` are specified
      then the `amount` can be omitted or it must be the sum of the amounts of all `subRates`.
    format: int16
    minimum: 0
    maximum: 1
    type: number
  includedInPrice:
    required: true
    type: boolean
  name:
    required: true
    type: string
  state:
    required: false
    description: |-
      The state in the country
    type: string
  subRates:
    required: false
    description: |-
      For countries (e.g.
      the US) where the total tax is a combination of multiple taxes (e.g.
      state and local taxes).
    type: array
    items:
      type: SubRate