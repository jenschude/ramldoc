#ByProjectKeyInventory
(resourcePathUri): /{projectKey}/inventory

uriParameters:
  projectKey:
    type: string
get:
  securedBy:
    oauth_2_0:
      scopes:
      - "manage_project:{projectKey}"
      - "view_products:{projectKey}"
  description: |-
    Query Inventory
  queryParameters:
    expand:
      required: false
      type: string
    where:
      required: false
      type: string
    sort:
      required: false
      type: string
    limit:
      required: false
      format: int16
      type: number
    offset:
      required: false
      format: int16
      type: number
    withTotal:
      required: false
      type: boolean
  responses:
    200:
      body:
        application/json:
          type: InventoryPagedQueryResponse
post:
  securedBy:
    oauth_2_0:
      scopes:
      - "manage_project:{projectKey}"
      - "manage_products:{projectKey}"
  description: |-
    Create InventoryEntry
  queryParameters:
    expand:
      required: false
      type: string
  body:
    application/json:
      type: InventoryEntryDraft
  responses:
    201:
      body:
        application/json:
          type: InventoryEntry
    200:
      body: