#ByProjectKeyMeSignup
(resourcePathUri): /{projectKey}/me/signup

uriParameters:
  projectKey:
    type: string
post:
  securedBy:
  - oauth_2_0:
      scopes:
      - "manage_my_profile:{projectKey}"
  body:
    application/json:
      type: MyCustomerDraft
  responses:
    201:
      body:
        application/json:
          type: CustomerSignInResult
    200:
      body: