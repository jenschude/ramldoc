# ByProjectKeyCartsByID
(resourceName): ByProjectKeyCartsByID
(resourcePathUri): /{projectKey}/carts/{ID}

uriParameters:
  projectKey:
    type: string
    (builtinType): string
  ID:
    type: string
    (builtinType): string
get:
  securedBy:
  - oauth_2_0:
      scopes:
      - "manage_project:{projectKey}"
      - "view_orders:{projectKey}"
  description: |-
    The cart may not contain up-to-date prices, discounts etc.
    If you want to ensure they're up-to-date, send an Update request with the Recalculate update action instead.
  queryParameters:
    expand:
      required: false
      type: string
      (builtinType): string
  responses:
    200:
      body:
        application/json:
          type: Cart
          (builtinType): object
post:
