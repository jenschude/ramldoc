  - oauth_2_0:
      scopes:
      - "manage_project:{projectKey}"
      - "manage_orders:{projectKey}"
  description: |-
    Update Cart by ID
  queryParameters:
    expand:
      required: false
      type: string
      (builtinType): string
  body:
