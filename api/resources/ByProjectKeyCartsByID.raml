#ByProjectKeyCartsByID
(resourcePathUri): /{projectKey}/carts/{ID}

uriParameters:
  projectKey:
    type: string
  ID:
    type: string
get:
  securedBy:
  - oauth_2_0:
      scopes:
      - "manage_project:{projectKey}"
      - "view_orders:{projectKey}"
  description: |-
    The cart may not contain up-to-date prices, discounts etc.
    If you want to ensure theyâ€™re up-to-date, send an Update request with the Recalculate update action instead.
  queryParameters:
    expand:
      required: false
      type: string
  responses:
    200:
      body:
        application/json:
          type: Cart
post:
