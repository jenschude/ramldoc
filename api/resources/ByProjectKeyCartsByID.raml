      type: CartUpdate
      (builtinType): object
  responses:
    200:
      body:
        application/json:
          type: Cart
          (builtinType): object
delete:
  securedBy:
  - oauth_2_0:
      scopes:
      - "manage_project:{projectKey}"
      - "manage_orders:{projectKey}"
  description: |-
    Delete Cart by ID
  queryParameters:
    dataErasure:
      required: false
      type: boolean
