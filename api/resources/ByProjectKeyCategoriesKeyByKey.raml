    Get Category by key
  queryParameters:
    expand:
      type: string
  responses:
    200:
      body:
        application/json:
          type: Category
post:
  description: |-
    Update Category by key
  queryParameters:
    expand:
      type: string
  body:
    application/json:
      type: CategoryUpdate
  responses:
    200:
      body:
        application/json:
          type: Category
delete:
  description: |-
    Delete Category by key
  queryParameters:
    version:
      format: int64
      type: number
    expand:
      type: string
  responses:
    200:
      body:
        application/json:
          type: Category