#ByProjectKeyInStoreKeyByStoreKeyMeOrders
(resourcePathUri): /{projectKey}/in-store/key={storeKey}/me/orders

uriParameters:
  projectKey:
    type: string
  storeKey:
    type: string
get:
  securedBy:
    oauth_2_0:
      scopes:
      - "manage_my_orders:{projectKey}"
      - "manage_my_orders:{projectKey}:{storeKey}"
  description: |-
    Query Orders
  queryParameters:
    expand:
      required: false
      type: string
    where:
      required: false
      type: string
    sort:
      required: false
      type: string
    limit:
      required: false
      format: int16
      type: number
    offset:
      required: false
      format: int16
      type: number
    withTotal:
      required: false
      type: boolean
  responses:
    200:
      body:
        application/json:
          type: OrderPagedQueryResponse
post:
  securedBy:
    oauth_2_0:
      scopes:
      - "manage_my_orders:{projectKey}"
      - "manage_my_orders:{projectKey}:{storeKey}"
  description: |-
    Create Order
  queryParameters:
    expand:
      required: false
      type: string
  body:
    application/json:
      type: MyOrderFromCartDraft
  responses:
    201:
      body:
        application/json:
          type: Order
