#ByProjectKeyMePasswordReset
(resourcePathUri): /{projectKey}/me/password/reset

uriParameters:
  projectKey:
    type: string
post:
  securedBy:
  - oauth_2_0:
      scopes:
      - "manage_my_profile:{projectKey}"
  responses:
    200:
      body:
        application/json:
          type: MyCustomer