(resourceName): ByProjectKey
(resourcePathUri): /{projectKey}
displayName: Project
description: |-
  The Project endpoint is used to retrieve certain information from a project.

uriParameters:
  projectKey:
    type: string
    (builtinType): string
get:
  securedBy:
  - oauth_2_0:
      scopes:
      - "manage_project:{projectKey}"
  description: |-
    The Endpoint is responding a limited set of information about settings and configuration of the project.
  responses:
    200:
      body:
        application/json:
          type: Project
          (builtinType): object
post:
  securedBy:
  - oauth_2_0:
      scopes:
      - "manage_project:{projectKey}"
  description: |-
    Update project
  body:
    application/json:
      type: ProjectUpdate
      (builtinType): object
  responses:
    200:
      body:
        application/json:
          type: Project
          (builtinType): object