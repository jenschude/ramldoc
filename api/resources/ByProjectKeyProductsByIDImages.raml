#ByProjectKeyProductsByIDImages
(resourcePathUri): /{projectKey}/products/{ID}/images

uriParameters:
  projectKey:
    type: string
  ID:
    type: string
post:
  securedBy:
  - oauth_2_0:
      scopes:
      - "manage_project:{projectKey}"
      - "manage_products:{projectKey}"
  description: |-
    Uploads a binary image file to a given product variant. The supported image formats are JPEG, PNG and GIF.
  queryParameters:
    filename:
      required: false
      type: string
    variant:
      required: false
      type: number
    sku:
      required: false
      type: string
    staged:
      required: false
      type: boolean
  body:
    application/json:
      fileTypes: [image/jpeg, image/png, image/gif]
      type: file
  responses:
    200:
      body:
        application/json:
          type: Product